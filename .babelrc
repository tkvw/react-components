{
    "presets": [
        ["@babel/preset-env",{
            "shippedProposals":true
        }],
        ["@babel/preset-react",{
            "development": true
        }]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "@babel/runtime"
        }
        ],"@babel/plugin-proposal-class-properties"]
}